<p>
<%= @comedian.name %>
<%= @venue %>
</p>

<% @events["_embedded"]["events"].each do |event| %>
<li><%= event["name"] %> - <%= event["dates"]["start"]["localDate"] %> <%= event["dates"]["start"]["localTime"] %> - <%= event["_embedded"]["venues"].first["name"] %> <%= event["_embedded"]["venues"].first["id"] %></li>
<% end %>

